{
  "name": "Kapla",
  "productName": "Kapla",
  "version": "1.0.3",
  "description": "Instrument, mit dem die Abteilung Recht des AWEL GeschÃ¤fte erfasst und Fristen kontrolliert",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-all": "npm run lint && npm run test && npm run build",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "npm run hot-server -- --start-hot",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "build-sqlite3-win64": "cd node_modules/sqlite3 && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v57-win32-x64 && node-gyp rebuild --target=1.7.5 --arch=x64 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.7-win32-x64",
    "build-sqlite3-mac": "cd node_modules/sqlite3 && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/node-v57-darwin-x64 && node-gyp rebuild --target=1.7.5 --arch=x64 --target_platform=darwin --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.7-darwin-x64"
  },
  "browserslist": "electron 1.4",
  "build": {
    "productName": "Kapla",
    "appId": "ch.zh.awel.kapla",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "category": "public.app-category.business"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chentsulin/electron-react-boilerplate.git"
  },
  "author": {
    "name": "Alexander Gabriel",
    "email": "alex@gabriel-software.ch",
    "url": "http://gabriel-software.ch"
  },
  "license": "MIT",
  "devDependencies": {
    "asar": "0.13.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "6.4.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-dev-expression": "0.2.1",
    "babel-plugin-tcomb": "0.3.25",
    "babel-plugin-webpack-loaders": "0.9.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-react-optimize": "1.0.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babili-webpack-plugin": "0.0.11",
    "boiler-room-custodian": "0.6.2",
    "chai": "4.1.1",
    "concurrently": "3.5.0",
    "cross-env": "5.0.5",
    "css-loader": "0.28.5",
    "devtron": "1.4.0",
    "electron": "1.7.5",
    "electron-builder": "15.4.3",
    "electron-devtools-installer": "2.2.0",
    "enzyme": "2.9.1",
    "eslint": "4.3.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-formatter-pretty": "1.1.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-compat": "1.0.2",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.1.0",
    "express": "4.15.4",
    "extract-text-webpack-plugin": "1.0.1",
    "fbjs-scripts": "0.8.1",
    "file-loader": "0.11.2",
    "imports-loader": "0.7.1",
    "html-webpack-plugin": "2.30.1",
    "jsdom": "11.2.0",
    "json-loader": "0.5.7",
    "minimist": "1.2.0",
    "mobx-react-devtools": "4.2.15",
    "mocha": "3.5.0",
    "react-addons-test-utils": "15.6.0",
    "sinon": "1.17.7",
    "spectron": "3.7.2",
    "style-loader": "0.18.2",
    "tcomb": "3.2.22",
    "url-loader": "0.5.9",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.18.1",
    "webpack-merge": "3.0.0",
    "webpack-validator": "2.3.0"
  },
  "dependencies": {
    "ampersand-app": "2.0.0",
    "electron-debug": "1.4.0",
    "exceljs": "0.6.2",
    "font-awesome": "4.7.0",
    "history": "4.7.2",
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "mobx": "3.3.1",
    "mobx-react": "4.3.3",
    "moment": "2.19.1",
    "react": "15.6.1",
    "react-bootstrap": "0.31.2",
    "react-bootstrap-daterangepicker": "~3.2.2",
    "react-dom": "15.6.1",
    "react-dropzone": "3.12.0",
    "react-linkify": "0.2.1",
    "react-split-pane": "0.1.66",
    "react-textarea-autosize": "5.1.0",
    "react-virtualized": "9.9.0",
    "recompose": "0.25.0",
    "source-map-support": "0.4.16",
    "sqlite3": "3.1.9",
    "styled-components": "2.1.2",
    "username": "~2.3.0"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
